<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/web-app_3_1.xsd"
           version="3.1">
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>src.Login</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>LoginAuth</servlet-name>
        <servlet-class>src.LoginAuth</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginAuth</servlet-name>
        <url-pattern>/loginAuth</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>MainPage</servlet-name>
        <servlet-class>src.MainPage</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MainPage</servlet-name>
        <url-pattern>/main</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>CreateAccount</servlet-name>
        <servlet-class>src.CreateAccount</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CreateAccount</servlet-name>
        <url-pattern>/createAccount</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>CreateAccountAuth</servlet-name>
        <servlet-class>src.CreateAccountAuth</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CreateAccountAuth</servlet-name>
        <url-pattern>/createAccountAuth</url-pattern>
    </servlet-mapping>

        <servlet>
        <servlet-name>DeleteAccount</servlet-name>
        <servlet-class>src.DeleteAccount</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DeleteAccount</servlet-name>
        <url-pattern>/deleteAccount</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>DeleteAccountAuth</servlet-name>
        <servlet-class>src.DeleteAccountAuth</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DeleteAccountAuth</servlet-name>
        <url-pattern>/deleteAccountAuth</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ChangePassword</servlet-name>
        <servlet-class>src.ChangePassword</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ChangePassword</servlet-name>
        <url-pattern>/changePassword</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ChangePasswordAuth</servlet-name>
        <servlet-class>src.ChangePasswordAuth</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ChangePasswordAuth</servlet-name>
        <url-pattern>/changePasswordAuth</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ListUsers</servlet-name>
        <servlet-class>src.ListUsers</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ListUsers</servlet-name>
        <url-pattern>/listUsers</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>LogoutAuth</servlet-name>
        <servlet-class>src.LogoutAuth</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LogoutAuth</servlet-name>
        <url-pattern>/logoutAuth</url-pattern>
    </servlet-mapping>

</web-app>

